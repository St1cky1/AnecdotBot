<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.24.xsd"
        objectQuotingStrategy="QUOTE_ONLY_RESERVED_WORDS">
    <changeSet id="1712407954669-1" author="BakSnn">
        <createSequence incrementBy="1" sequenceName="jokes_id_seq"/>
    </changeSet>
    <changeSet id="1712407954669-2" author="BakSnn">
        <validCheckSum>ANY</validCheckSum>
        <createTable tableName="jokes">
            <column autoIncrement="true" name="id" type="bigint">
                <constraints nullable="false" primaryKey="true" primaryKeyName="jokes_pkey"/>
            </column>
            <column name="joke" type="VARCHAR(255)"/>
            <column name="time_created" type="timestamp"/>
            <column name="time_updated" type="timestamp"/>
        </createTable>
    </changeSet>
    <changeSet id="1712407954669-3" author="BakSnn">
        <createSequence incrementBy="1" sequenceName="users_call_id_seq"/>
    </changeSet>
    <changeSet id="1712407954669-4" author="BakSnn">
        <validCheckSum>ANY</validCheckSum>
        <createTable tableName="users">
            <column autoIncrement="true" name="id" type="bigint">
                <constraints nullable="false" primaryKey="true" primaryKeyName="users_call_pkey"/>
            </column>
            <column name="joke_id" type="bigint">
                <constraints nullable="false"/>
            </column>
            <column name="call_time" type="timestamp"/>
            <column name="user_id" type="bigint"/>
            <column name="joke_text" type="VARCHAR(255)"/>
        </createTable>
    </changeSet>
    <changeSet id="1712407954669-5" author="BakSnn">
        <addForeignKeyConstraint baseColumnNames="joke_id"
                                 baseTableName="users_call"
                                 constraintName="fk_users_call_joke_id"
                                 referencedColumnNames="id"
                                 referencedTableName="jokes"/>
    </changeSet>
    <changeSet id="1712407954669-6" author="BakSnn">
        <createSequence incrementBy="1" sequenceName="clients_id_seq"/>
    </changeSet>
    <changeSet id="1712407954669-7" author="BakSnn">
        <validCheckSum>ANY</validCheckSum>
        <createTable tableName="clients">
            <column autoIncrement="true" name="id" type="bigint">
                <constraints nullable="false" primaryKey="true" primaryKeyName="clients_pkey"/>
            </column>
            <column name="username" type="VARCHAR(255)"/>
            <column name="password" type="VARCHAR(255)"/>
            <column name="expired" type="boolean"/>
            <column name="locked" type="boolean"/>
            <column name="enabled" type="boolean"/>
        </createTable>
    </changeSet>
</databaseChangeLog>